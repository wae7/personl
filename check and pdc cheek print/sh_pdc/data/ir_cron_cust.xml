<?xml version ="1.0" encoding="utf-8" ?>
<odoo>
    <record id="pdc_cheque_due_date_notify_customer" model="ir.cron">
        <field name="name">PDC Cheque Due Date Notification Customer</field>
        <field eval="True" name="active" />
        <field name="user_id" ref="base.user_root" />
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="model_id" ref="sh_pdc.model_pdc_wizard" />
        <field name="state">code</field>
        <field name="code">model.notify_customer_due_date()</field>
        <field name="priority" eval="5" />
    </record>
</odoo>
